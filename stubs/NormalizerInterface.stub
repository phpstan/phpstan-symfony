<?php

namespace Symfony\Component\Serializer\Normalizer;

interface NormalizerInterface
{
    /**
     * @param mixed $object
     * @param string $format
     * @param mixed[] $context
     *
     * @return mixed[]|string|int|float|bool|null
     */
    public function normalize($object, $format = null, array $context = []);
}
